<!DOCTYPE html>
<html>
<head>
  <title></title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="/js/jquery.js" type="text/javascript"></script>
</head>
<body>
  <div id="phone">
    <div id="content">
    </div>
  </div>
</body>
<style>
  html,body{
    margin:0;
    padding:0;
    overflow: hidden;
  }

  #phone{
    width: 375px;
    height: 792px;
    margin:0 auto;
    background:url('images/phone-case.png') no-repeat;
    position:relative;
  }
  #content{
    width: 328px;
    height: 576px;
    overflow: scroll;
    background-color: #fff;
    position: absolute;
    top:100px;
    left:15px;
  }
</style>
<script>
$(function(){
  var content = sessionStorage.getItem("articleEditor");
  $("#content").html(content);
});
</script>
</html>
